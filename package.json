{
  "name": "react-rollup-small-boilerplate",
  "version": "0.0.1",
  "description": "Minimal & Small React boilerplate with Rollup",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c --perf",
    "serve": "NODE_ENV=development rollup -c -w"
  },
  "keywords": [
    "react",
    "rollup"
  ],
  "author": "Muhammad K. Huda",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "postcss": "^8.3.8",
    "rollup": "^2.57.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkhuda/react-rollup-small-boilerplate.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 9"
    ]
  }
